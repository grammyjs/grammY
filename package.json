{
    "name": "grammy",
    "description": "The Telegram Bot Framework.",
    "version": "1.36.1",
    "author": "KnorpelSenf",
    "license": "MIT",
    "engines": {
        "node": "^12.20.0 || >=14.13.1"
    },
    "homepage": "https://grammy.dev/",
    "repository": {
        "type": "git",
        "url": "https://github.com/grammyjs/grammY"
    },
    "scripts": {
        "prepare": "npm run backport",
        "backport": "deno2node tsconfig.json"
    },
    "dependencies": {
        "@grammyjs/types": "3.20.0",
        "abort-controller": "^3.0.0",
        "debug": "^4.3.4",
        "node-fetch": "^2.7.0"
    },
    "devDependencies": {
        "@types/debug": "^4.1.12",
        "@types/node": "^12.20.55",
        "@types/node-fetch": "2.6.2",
        "deno2node": "^1.13.0"
    },
    "files": [
        "out/"
    ],
    "main": "./out/mod.js",
    "types": "./out/mod.d.ts",
    "exports": {
        ".": {
            "types": "./out/mod.d.ts",
            "node": "./out/mod.js",
            "browser": "./out/web.mjs",
            "default": "./out/web.mjs"
        },
        "./types": {
            "types": "./out/types.d.ts",
            "default": "./out/types.js"
        },
        "./web": {
            "types": "./out/web.d.ts",
            "default": "./out/web.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "web": [
                "out/web"
            ],
            "types": [
                "out/types"
            ]
        }
    },
    "keywords": [
        "telegram",
        "bot",
        "api",
        "client",
        "framework",
        "library",
        "grammy"
    ]
}
